{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "src/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "index.js", "methods": ["GET","POST","PUT","DELETE","OPTIONS","PATCH"] },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}
